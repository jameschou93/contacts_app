<h2>Hello <%= current_user.name %></h2>

<table>
  <thead>
    <tr>
    <th> ID </th>
    <th> Full Name</th>
    <th>First Name</th>
    <th>Middle Name</th>
    <th>Last Name</th>
    <th>Email</th>
    <th>phone</th>
    <th>Address</th>
    <th>Latitude</th>
    <th>Longitude</th>
 

    </tr>
  </thead>
  <tbody>
  <% @contacts.each do |contact| %>
 
      <tr>
        <td> <%= contact.id %></td>
        <td><%= contact.full_name %></td>
        <td><%= contact.first_name %></td>
        <td> <%= contact.last_name %></td>
        <td> <%= contact.email %></td>
        <td> <%= contact.jap_phone %></td>
        <td><%= contact.friendly_time %></td>
        <td><%= contact.address %></td>
        <td><%= contact.latitude %></td>
        <td><%= contact.longitude %></td>
      </tr>

    

    <% end %>
  </tbody>
</table>
